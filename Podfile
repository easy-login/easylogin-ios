# platform :ios, '9.0'

target 'EasyLoginSDK' do
    # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!
    
    # Pods for EasyLoginSDK
    pod 'LineSDKSwift', '~> 5.0'
    target 'EasyLoginSDKTests' do
        inherit! :search_paths
        # Pods for testing
    end
    
end

